{
  "name": "sitemapservice",
  "version": "1.0.0",
  "description": "sitemap service",
  "private": true,
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bauerxcelmedia/sitemap-service.git"
  },
  "scripts": {
    "migrate": "node ./node_modules/sequelize-cli/bin/sequelize db:migrate",
    "migrate:undo": "node ./node_modules/sequelize-cli/bin/sequelize db:migrate:undo",
    "migrate:create": "node ./node_modules/sequelize-cli/bin/sequelize migration:create",
    "coverage": "node ./node_modules/babel-istanbul/lib/cli cover --root ./app --include-all-sources --dir ./coverage --report cobertura --report html --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --ui bdd --compilers js:babel/register tests/setup.js tests/ --recursive",
    "lint": "node ./node_modules/eslint/bin/eslint.js --ext .js app/",
    "start": "npm run migrate && node index.js",
    "test": "node ./node_modules/mocha/bin/_mocha -t 10000 --ui bdd --compilers js:babel/register tests/setup.js tests/ --recursive",
    "test:unit": "node ./node_modules/mocha/bin/_mocha --ui bdd --compilers js:babel/register tests/setup.js tests/unit/ --recursive",
    "test:integration": "node ./node_modules/mocha/bin/_mocha --ui bdd --compilers js:babel/register tests/setup.js tests/integration/ --recursive",
    "tdd": "npm run test -- -w",
    "build": "npm run lint"
  },
  "dependencies": {
    "@bxm/eslint-config-microservice": "0.0.2",
    "@bxm/microservice": "^1.0.2",
    "@bxm/node-apm": "^1.0.1",
    "@bxm/pro-amqplib": "0.0.7",
    "@bxm/winston-logger": "^1.0.19",
    "amqplib": "^0.4.0",
    "babel": "^5.3.0",
    "config": "^1.19.0",
    "express": "^4.12.3",
    "keymirror": "^0.1.1",
    "lodash": "^3.9.3",
    "moment": "^2.12.0",
    "odata-parser": "^1.2.1",
    "pg": "^4.4.3",
    "pg-hstore": "^2.3.2",
    "request": "^2.57.0",
    "sequelize": "^3.17.3",
    "sequelize-cli": "^2.3.1",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-istanbul": "0.2.6",
    "babel-runtime": "^6.5.0",
    "chai": "^2.1.1",
    "eslint": "^0.23.0",
    "mocha": "^2.2.3",
    "nock": "^2.7.0",
    "proxyquire": "^1.5.0",
    "sinon": "^1.15.3",
    "superagent": "^1.2.0",
    "supertest": "^1.1.0"
  }
}
